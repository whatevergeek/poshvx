{
    "name": "Microsoft.WSMan.Runtime",
    "version": "1.0.0-*",

    "buildOptions": {
        "keyFile": "../signing/visualstudiopublic.snk",
        "delaySign": true,
        "publicSign": false,
        "warningsAsErrors": true
    },

    "frameworks": {
        "netstandard1.6": {
            "imports": [ "dnxcore50" ],
            "buildOptions": {
                "define": [ "CORECLR" ],
                "debugType": "portable"
            },
            "dependencies": {
                "NETStandard.Library": "1.6.0",
            }
        },
        "net451": {
            "frameworkAssemblies": {
                "System.Runtime": "",
                "System.Xml": "",
                "System.Data": ""
            }
        }
    }
}
